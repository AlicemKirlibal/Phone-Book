@model IEnumerable<PhoneBook.WebUI.Models.Employee>
@{
    ViewBag.Title = "Home Page";
}



    <br />
<table class="table">
    <thead>
        <tr>
            <th scope="col">Name</th>
            <th scope="col">City</th>
            <th scope="col">Phone Number</th>
            <th scope="col">Details</th>
        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model)
        {
            <tr>
                <td>@item.FullName</td>
                <td>@item.City</td>
                <td>@item.PhoneNumber</td>
                <td><a href="/Home/Details/@item.Id" class="btn btn-warning">Details</a></td>
           
            </tr>
        }



    </tbody>
</table>